{{#each items as |item key|}}
<li class="custom-dnd5e-item flexrow" data-key="{{customDnd5eDotNotateChild ../parentKey key}}">
    <i class="custom-dnd5e-drag flex0 fas fa-grip-lines" draggable="true"></i>
    <input id="visible" name="{{customDnd5eDotNotateChild ../parentKey key}}.visible" type="checkbox" data-tooltip="{{localize "CUSTOM_DND5E.form.checkbox.visible.tooltip"}}"{{#unless (customDnd5eBoolFalse visible)}} checked{{/unless}}>
    <div class="fields flexrow">
        <input id="parentKey" name="{{customDnd5eDotNotateChild ../parentKey key}}.parentKey" type="hidden" value="{{../parentKey}}">
        <input id="key" name="{{customDnd5eDotNotateChild ../parentKey key}}.key" type="hidden" value="{{key}}">
        <input id="system" name="{{customDnd5eDotNotateChild ../parentKey key}}.system" type="hidden" value="{{#if (customDnd5eBoolFalse system)}}false{{else}}true{{/if}}">
        <input id="label" name="{{customDnd5eDotNotateChild ../parentKey key}}.label" type="text" value="{{localize label}}"> 
    </div>
    <button{{#unless (customDnd5eBoolFalse system)}} class="hidden"{{/unless}} type="button" data-tooltip="{{localize "CUSTOM_DND5E.form.button.delete.tooltip"}}" data-action="delete" class="flex0 custom-dnde-list-button">
        <i class="fas fa-xmark"></i>
    </button>
    <input id="delete" name="{{customDnd5eDotNotateChild ../parentKey key}}.delete" type="hidden" value="false">
    {{#if item.children}}
    <ul class="custom-dnd5e-list flexcol">
        {{> "modules/custom-dnd5e/templates/config-list.hbs" prefix=../prefix parentKey=(customDnd5eDotNotateChild ../parentKey key) items=item.children }}
    </ul>
    {{/if}}
</li>
{{/each}}